						  ; something like ".ddd"
 000000C8  D9 EE		      fldz	  ; ONE thing in the FPU stack
 000000CA  DD 05 00000123 R	      fld  ten
 000000D0  DD 1D 00000049 R	      fstp power
				      .WHILE (al >= '0' && al <= '9')
